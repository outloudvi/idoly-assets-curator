{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.txt",
            "use": "@vercel/static"
        },
        {
            "src": "vercel/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/img/(.*)",
            "dest": "/vercel/img.py"
        },
        {
            "src": "/api/env/(.*)",
            "dest": "/vercel/env.py"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/404.html"
        }
    ],
    "github": {
        "silent": true
    }
}